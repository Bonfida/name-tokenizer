{
  "name": "@bonfida/name-tokenizer",
  "version": "0.0.9",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --detectOpenHandles --coverage",
    "dev": "tsc && node dist/test.js",
    "prebuild": "rm -rf dist",
    "build": "rollup -c",
    "build:rm": "rollup -c && rm -rf node_modules",
    "prepublish": "rm -rf dist && rollup -c",
    "lint": "yarn pretty && eslint .",
    "lint:fix": "yarn pretty:fix && eslint . --fix",
    "pretty": "prettier --check 'src/*.[jt]s'",
    "pretty:fix": "prettier --write 'src/*.[jt]s'",
    "doc": "yarn typedoc src/index.ts"
  },
  "devDependencies": {
    "@bonfida/spl-name-service": "^0.2.7",
    "@metaplex-foundation/mpl-token-metadata": "^1.2.5",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "^1.73.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.14.20",
    "@types/tmp": "^0.2.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^27.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rollup": "^2.75.7",
    "rollup-plugin-terser": "^7.0.2",
    "save-dev": "0.0.1-security",
    "tmp": "^0.2.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.22.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@bonfida/spl-name-service": "^0.2.7",
    "@solana/spl-token": "^0.3.7",
    "bn.js": "^5.2.0",
    "borsh": "^0.6.0",
    "bs58": "4.0.1",
    "buffer": "^6.0.3"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.73.2"
  }
}
