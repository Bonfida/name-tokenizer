{
  "name": "@bonfida/name-tokenizer",
  "version": "0.0.14",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --detectOpenHandles --coverage",
    "dev": "tsc && node dist/test.js",
    "prebuild": "rm -rf dist",
    "build": "rollup -c",
    "build:rm": "rollup -c && rm -rf node_modules",
    "prepublish": "rm -rf dist && rollup -c",
    "lint": "yarn pretty && eslint .",
    "lint:fix": "yarn pretty:fix && eslint . --fix",
    "pretty": "prettier --check 'src/*.[jt]s'",
    "pretty:fix": "prettier --write 'src/*.[jt]s'",
    "doc": "yarn typedoc src/index.ts"
  },
  "devDependencies": {
    "@bonfida/spl-name-service": "^1.5.0",
    "@bonfida/utils": "^0.0.7",
    "@metaplex-foundation/mpl-token-metadata": "^1.2.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "^1.87.3",
    "@tsconfig/recommended": "^1.0.3",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^14.14.20",
    "@types/tmp": "^0.2.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rollup": "^4.3.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "save-dev": "0.0.1-security",
    "tmp": "^0.2.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@solana/spl-token": "^0.3.11",
    "borsh": "2.0.0",
    "buffer": "^6.0.3"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.87.3"
  }
}
